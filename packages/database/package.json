{
  "name": "@nyuchi/database",
  "version": "1.0.0",
  "description": "Supabase database client and queries for Nyuchi Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "generate-types": "npx supabase gen types typescript --linked > src/types.generated.ts"
  },
  "keywords": ["supabase", "database", "nyuchi", "postgres"],
  "dependencies": {
    "@nyuchi/ubuntu": "*",
    "@supabase/supabase-js": "^2.49.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  }
}
